         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="65.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@plus.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="removeUser" alignment="CENTER" layoutX="823.0" layoutY="92.0" mnemonicParsing="false" onAction="#addApp" prefHeight="39.0" prefWidth="37.0" style="-fx-background-color: #ababab;">
         <font>
            <Font size="1.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@delete.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="706.0" layoutY="94.0" text="Users">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
