<AnchorPane onMouseClicked="#backgroundClicked" prefHeight="800.0" prefWidth="1000.0" style="-fx-background-color: #C0C0C0;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainViewController">
   <children>
      <Pane prefHeight="800.0" prefWidth="222.0" style="-fx-background-color: #ff8c00;">
         <children>
            <ImageView fitHeight="113.0" fitWidth="120.0" layoutX="49.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@key.png" />
               </image>
            </ImageView>
            <Label layoutX="12.0" layoutY="151.0" prefHeight="61.0" prefWidth="216.0" text="Password Manager">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
            </Label>
            <Line endX="-16.5" layoutX="106.0" layoutY="230.0" startX="-106.0" stroke="#838383" strokeLineJoin="ROUND" />
            <Line endX="64.5" layoutX="110.0" layoutY="548.0" startX="-110.0" stroke="#838383" />
            <Text layoutX="18.0" layoutY="267.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="175.5" />
         </children>
      </Pane>
      <Label layoutX="258.0" layoutY="89.0" text="Applications">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <ListView fx:id="appsList" layoutX="258.0" layoutY="136.0" onMouseClicked="#selectedApp" prefHeight="621.0" prefWidth="246.0" style="-fx-background-color: #ababab;" />
      <Button fx:id="addApp" alignment="CENTER" layoutX="412.0" layoutY="88.0" mnemonicParsing="false" onAction="#addApp" prefHeight="39.0" prefWidth="37.0" style="-fx-background-color: #ababab;">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="65.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@plus.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="removeApp" alignment="CENTER" layoutX="456.0" layoutY="88.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="37.0" style="-fx-background-color: #ababab;">
         <font>
            <Font size="1.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@delete.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <TableView fx:id="tableView" layoutX="524.0" layoutY="136.0" onMouseClicked="#tableClicked" prefHeight="322.0" prefWidth="435.0" style="-fx-background-color: #ababab;">
        <columns>
          <TableColumn fx:id="usernameColumn" prefWidth="168.0" resizable="false" text="Username" />
          <TableColumn fx:id="passwordColumn" prefWidth="171.0" resizable="false" text="Password" />
            <TableColumn fx:id="lastModifiedColumn" prefWidth="95.0" resizable="false" text="Last Modified" />
        </columns>
      </TableView>
      <Button fx:id="modifyPassword" disable="true" layoutX="652.0" layoutY="527.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="136.0" text="Modify Password" />
      <Label fx:id="passwordStrengthLabel" contentDisplay="RIGHT" disable="true" layoutX="619.0" layoutY="472.0" prefHeight="55.0" prefWidth="201.0" text="Password Strength:">
         <font>
            <Font size="23.0" />
